{
 "name": "Meditation Chrome Extension",
 "description": "Meditation Reminder",
 "version": "1.0",
 "manifest_version": 2,
 "background": {
     "scripts": ["background.js"],
     "persistent": false
  },
 "permissions": ["notifications"],
 "browser_action": {
     "default_popup": "popup.html",
     "default_icon": {
         "128": "waking-up.png"
     }
    },
"content_scripts": [
{
    "js": ["content-script.js"],
    "matches": [
        "<all_urls>"
    ]
}
]
}

