{
    "name": "Meditation Chrome Extension",
    "description": "Meditation Reminder",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "notifications"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}